<script type="text/javascript" src="js/panel.js"></script>
<script type="text/javascript" src="js/highcharts.js"></script>
<script type="text/javascript" src="js/jquery.event.ue.js"></script>
<script type="text/javascript" src="js/jquery.udraggable.js"></script>
<script type="text/javascript" src="js/select-ui.min.js"></script>
</head>
<body>
	<div id="continer" style="margin: 0px auto;">
		<div id="head"></div>
		<div id="left" style="float:left;width:200px;"></div>
		<div id="right"></div>
	</div>
</body>
</html>
