<script type="text/javascript" src="js/jquery.js"></script>
<script type="text/javascript" src="js/jquery-ui.js"></script>
<script async defer src="http://maps.google.cn/maps/api/js?key=AIzaSyAZXJKfpk2QOlYQcKhzTUpKUr-ge3FbEBc&callback=initMap">
</script>
<script type="text/javascript">
	$(document).ready(function() {
		$('#head').load("head.html");
		$('#left').load("menuli.html",
			function() {
				$('#menu').metisMenu();
				
				//Load right page based on model
				var model = GetQueryString("model");
				loadByModel(model);		
				
				$("#menu>li>ul>li").click(function() {
					$("#menu>li>ul>li").removeClass("activeli");
					$(this).addClass("activeli");
					var model = $(this).children("a").attr("model");
					var target = $(this).children("a").attr("target");
					$("#right").load(target, function() {
						doCallback(eval(model), [ model ]);
					});
				});

				$("#menu>li").click(function() {
					$("#menu>li").removeClass("activefirstli");
					$(this).addClass("activefirstli");
				});

				$(
						'<audio id="chatAudio"><source src="audio/alarm.ogg" type="audio/ogg"><source src="audio/alarm.mp3" type="audio/mpeg"><source src="audio/alarm.wav" type="audio/wav"></audio>')
						.appendTo('#alermaudioDiv');
				$(
						'<audio id="seekhelpAudio"><source src="audio/seekHelp.ogg" type="audio/ogg"><source src="audio/seekHelp.mp3" type="audio/mpeg"><source src="audio/seekHelp.wav" type="audio/wav"></audio>')
						.appendTo('#seekhelpaudioDiv');

				// setInterval("miotdevice()", 5000);
				// setInterval("monitorSeekHelp()", 3000);
				$("#alarm-message").click(function() {
					var firstMenu = "menuManager";
					var secondMenu = "alarmManager";
					changeMenu(firstMenu, secondMenu);
					/*var model = $("#alarmManager").children("a").attr("model");
					var target = $("#alarmManager").children("a").attr("target");*/
					var model = $("#" + secondMenu).children("a").attr("model");
					var target = $("#" + secondMenu).children("a").attr("target");
					$("#right").load(target, function() {
						doCallback(eval(model), [ model ]);
					});
				});
				$("#seekhelp-message").click(function() {
					var firstMenu = "menuManager";
					var secondMenu = "seekHelpManager";
					changeMenu(firstMenu, secondMenu);
					var model = $("#" + secondMenu).children("a").attr("model");
					var target = $("#" + secondMenu).children("a").attr("target");
					$("#right").load(target, function() {
						doCallback(eval(model), [ model ]);
						// doCallbackOfArgs(model, [ ]);
					});
				});

			});
	});
</script>

<script type="text/javascript" src="js/metisMenu.js"></script>
<script type="text/javascript" src="js/dialog.js"></script>
